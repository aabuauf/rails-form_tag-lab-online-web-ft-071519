<h3>Student Form</h3>
<%= form_tag students_path do %>
  
  <input type="text" id="student_firstname" name="student[first_name]"><br>
  <input type="text" id="student_lastname" name="student[last_name]"><br>
 

 
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <input type="submit" value="Submit Post">
<% end %>